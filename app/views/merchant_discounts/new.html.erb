<%= form_with url: merchant_discounts_path, method: "post", local: true do |f| %>
  <%= f.label :discount %>
  <%= f.number_field :discount, min:0.05, max:1.0, step: 0.05 %><br><br>
  <%= f.label :threshold %>
  <%= f.number_field :threshold, min:10, max:50, step: 5 %><br><br>
  <%= f.submit "Submit" %>
<% end  %>
